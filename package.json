{
  "name": "bclub-tools",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm-run-all build:popup --parallel build:manifests build:background build:content-script",
    "build-debug": "npm-run-all build-debug:popup --parallel build:manifests build-debug:background build-debug:content-script",
    "build:manifests": "node ./projects/manifest/create-manifests.js",
    "build:popup": "ng build --aot --configuration production --output-hashing=none --source-map=false",
    "build:background": "webpack-cli --config ./projects/background/webpack.config.js --context ./projects/background/ --mode=production",
    "build:content-script": "webpack-cli --config ./projects/content-script/webpack.config.js --context ./projects/content-script/ --mode=production",
    "build-debug:popup": "ng build --aot",
    "build-debug:background": "webpack-cli --config ./projects/background/webpack.config.js --context ./projects/background/ --mode=development",
    "build-debug:content-script": "webpack-cli --config ./projects/content-script/webpack.config.js --context ./projects/content-script/ --mode=development",
    "tool:update-externals": "node ./tools/update-externals.js 0d6afacd64e53bd4ec0e8f7698eb47e1352938cb",
    "package": "node ./release/create-package.js",
    "test": "ng test",
    "test:ci": "npm run test -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
    "@angular/core": "^15.2.7",
    "@angular/forms": "^15.2.7",
    "@angular/material": "^15.2.5",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@angular/router": "^15.2.7",
    "jszip": "^3.10.1",
    "lz-string": "^1.5.0",
    "papaparse": "^5.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.7",
    "@angular/language-service": "^15.2.7",
    "@types/chrome": "~0.0.227",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.11.7",
    "@types/sinon-chrome": "^2.2.11",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "archiver": "^3.1.1",
    "eslint": "^8.33.0",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-mocks": "^14.8.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "sinon-chrome": "^3.0.1",
    "socket.io-client": "^4.6.1",
    "ts-loader": "^9.4.2",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.5",
    "webpack-cli": "^5.0.1"
  }
}