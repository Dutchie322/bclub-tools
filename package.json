{
  "name": "bclub-tools",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm-run-all build:popup --parallel build:manifests build:background build:content-script",
    "build-debug": "npm-run-all build-debug:popup --parallel build:manifests build-debug:background build-debug:content-script",
    "build:manifests": "node ./projects/manifest/create-manifests.js",
    "build:popup": "ng build --aot --configuration production --output-hashing=none --source-map=false",
    "build:background": "webpack-cli --config ./projects/background/webpack.config.js --context ./projects/background/ --mode=production",
    "build:content-script": "webpack-cli --config ./projects/content-script/webpack.config.js --context ./projects/content-script/ --mode=production",
    "build-debug:popup": "ng build --aot",
    "build-debug:background": "webpack-cli --config ./projects/background/webpack.config.js --context ./projects/background/ --mode=development",
    "build-debug:content-script": "webpack-cli --config ./projects/content-script/webpack.config.js --context ./projects/content-script/ --mode=development",
    "tool:update-externals": "node ./tools/update-externals.js 313c34760354be34b45d5835044a946943103010",
    "package": "node ./release/create-package.js",
    "test": "ng test",
    "test:ci": "npm run test -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "jszip": "^3.10.1",
    "lz-string": "^1.5.0",
    "papaparse": "^5.4.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular-eslint/builder": "^17.1.1",
    "@angular-eslint/eslint-plugin": "^17.1.1",
    "@angular-eslint/eslint-plugin-template": "^17.1.1",
    "@angular-eslint/schematics": "^17.1.1",
    "@angular-eslint/template-parser": "^17.1.1",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@angular/language-service": "^17.0.8",
    "@types/chrome": "~0.0.254",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^20.10.5",
    "@types/sinon-chrome": "^2.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "archiver": "^6.0.1",
    "eslint": "^8.53.0",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-mocks": "^14.12.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "sinon-chrome": "^3.0.1",
    "socket.io-client": "^4.7.2",
    "ts-loader": "^9.5.1",
    "ts-node": "~10.9.2",
    "typescript": "~5.2.2",
    "webpack-cli": "^5.1.4"
  }
}