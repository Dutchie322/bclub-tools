{
  "name": "bclub-tools",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm-run-all build:popup --parallel build:manifests build:background build:content-script",
    "build-debug": "npm-run-all build-debug:popup --parallel build:manifests build-debug:background build-debug:content-script",
    "build:manifests": "node ./projects/manifest/create-manifests.js",
    "build:popup": "ng build --aot --prod --output-hashing=none --source-map=false",
    "build:background": "webpack-cli --config ./projects/background/webpack.config.js --context ./projects/background/ --mode=production",
    "build:content-script": "webpack-cli --config ./projects/content-script/webpack.config.js --context ./projects/content-script/ --mode=production",
    "build-debug:popup": "ng build --aot",
    "build-debug:background": "webpack-cli --config ./projects/background/webpack.config.js --context ./projects/background/ --mode=development",
    "build-debug:content-script": "webpack-cli --config ./projects/content-script/webpack.config.js --context ./projects/content-script/ --mode=development",
    "tool:update-externals": "node ./tools/update-externals.js 0d6afacd64e53bd4ec0e8f7698eb47e1352938cb",
    "package": "node ./release/create-package.js",
    "test": "ng test",
    "test:ci": "npm run test -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~10.2.5",
    "@angular/compiler": "~10.2.5",
    "@angular/core": "~10.2.5",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "~10.2.5",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~10.2.5",
    "@angular/platform-browser-dynamic": "~10.2.5",
    "@angular/router": "~10.2.5",
    "jszip": "^3.8.0",
    "lz-string": "^1.4.4",
    "papaparse": "^5.2.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.4",
    "@angular/cli": "~10.2.4",
    "@angular/compiler-cli": "~10.2.5",
    "@angular/language-service": "~10.2.5",
    "@types/chrome": "0.0.86",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/sinon-chrome": "^2.2.10",
    "archiver": "^3.1.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-mocks": "^11.9.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "sinon-chrome": "^3.0.1",
    "socket.io-client": "^4.2.0",
    "ts-loader": "^6.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8",
    "webpack-cli": "^3.3.5"
  }
}
